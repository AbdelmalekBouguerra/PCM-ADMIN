<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
  <title>Corona Admin</title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="/vendors/mdi/css/materialdesignicons.min.css" />
  <link rel="stylesheet" href="/vendors/css/vendor.bundle.base.css" />

  <!-- Layout styles -->
  <link rel="stylesheet" href="/css/style.css" />
  <!-- End layout styles -->
  <link rel="shortcut icon" href="/images/favicon.png" />

  <link href="/vendors/tabulator-master/dist/css/tabulator_midnight.min.css" rel="stylesheet">

</head>

<body>
  <div class="container-scroller">
    <!-- sidebar partial -->
    {{> _sidebar}}

    <div class="container-fluid page-body-wrapper">
      <!-- navbar partial -->
      {{> _navbar}}
      <div class="main-panel">
        <!-- content-wrapper -->
        <div class="content-wrapper">
          {{!-- page title --}}
          <div class="page-header">
            <h3 class="page-title">Structures SH</h3>
          </div>

          <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <p class="card-description">Suivant la décision portant codes des structures SH N° 337 /DG du
                    07/09/2021
                  </p>
                  <div class="nav-link mt-2 mt-md-0 d-none d-lg-flex search">
                    <select class="form-control" style="width: 200px;" id="filter-field">
                      <option value="CODE" selected>Code Mnémonique</option>
                      <option value="STRUCTURE">Structures</option>
                    </select>
                    <input type="text" id="filter-value" class="form-control" placeholder="Search products">
                  </div>
                  <div id="example-table"></div>

                  <button type="button" class="btn btn-inverse-success btn-fw m-2" id="add-row">
                    <i class="mdi mdi-plus">Ajouter</i>
                  </button>
                  <form method="post" action="/deleteSH" onsubmit='if (!confirm("êtes-vous sûr de vouloir supprimer ces lignes!")) {
                    return false;
                    }'>
                    <input type="hidden" name="selectedRowsForDel" id="selectedRowsForDel" />
                    <button id="del-row" type="submit" class="btn btn-inverse-danger btn-fw">Supprimer</button>
                  </form>
                  <br>
                  <div id="btn"></div>
                </div>
              </div>
            </div>
            {{!-- <div class="col-lg-6 grid-margin stretch-card align-content-center" id="btn"></div> --}}
          </div>
        </div>
        <!-- content-wrapper ends -->

        <!-- footer partial -->
        {{> _footer}}
      </div>
      <!-- main-panel ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <!-- plugins:js -->
  <script src="/vendors/js/vendor.bundle.base.js"></script>
  <!-- endinject -->

  <!-- inject:js -->
  <script src="/js/off-canvas.js"></script>
  <script src="/js/hoverable-collapse.js"></script>
  <script src="/js/misc.js"></script>
  <script src="/js/settings.js"></script>
  <script src="/js/todolist.js"></script>
  <!-- endinject -->
  <!-- Custom js for this page -->

  <script type="text/javascript" src="/vendors/tabulator-master/dist/js/tabulator.min.js"></script>

  <script src="/js/tables/sh_table.js"></script>

  <!-- End custom js for this page -->
</body>

</html>